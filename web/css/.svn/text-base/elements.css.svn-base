/* BUTTONS */
.button{
    border-radius: 3px;
    box-shadow: 0 6px 10px -4px #A2A2A2;
    display: block;
    float: left;
    font-family: Ubuntu, sans-serif;
    font-size: 1.1em;
    font-weight: 500;
    height: 30px;
    line-height: 30px;
    padding: 0 12px;
    text-indent: 0;
    
    -moz-box-shadow: 0 6px 10px -4px #A2A2A2;
    -webkit-box-shadow: 0 6px 10px -4px #A2A2A2;
    -ms-box-shadow: 0 6px 10px -4px #A2A2A2;
    -o-box-shadow: 0 6px 10px -4px #A2A2A2;
}
.button:focus, .button:hover, .button:active{
    box-shadow: 0 6px 10px -4px #353535;
}
.button.blue{
    background: #2C75A6;
    color: #FFF;
}
.button.blue:focus, .button.blue:hover, .button.blue:active{
    background: #6BB4D9;
}
.button.orange{
    background: #F88C13;
    color: #FFF;
}
.button.orange:focus, .button.orange:hover, .button.orange:active{
    background-color: #F7B244 !important;
}

.central-buttons .button.big-orange{
    background: url("../graphics/website/sprites/icons.png") no-repeat scroll 217px -1059px #f88c13;
    border-radius: 50px;
    color: #fff;
    float: right;
    font-size: 1.7em;
    height: 50px;
    margin-bottom: 10px;
    padding: 0 32px 10px;
    text-align: center;
    width: 200px;
}
.central-buttons .button.big-orange:focus, .central-buttons .button.big-orange:hover, .central-buttons .button.big-orange:active{
    background-color: #F7B244 !important;
}


.button.gray{
    background: #EBEBEB;
    color: #000;
}
.button.gray.dark{
    background: #E7E7E7;
}
.button.gray:focus, .button.gray:hover, .button.gray:active{
    background-color: #F7B244;
    color: #FFF;
}
.button.a_icon_delete:focus, .button.a_icon_delete:hover, .button.a_icon_delete:active{
    background-color: #F00 !important;
}
.button.laquo:before{
    content: "\00ab\00A0";
}
.button.raquo:after{
    content: "\00A0\00bb";
}
.button.up.raquo:after,
.button.down.raquo:after{
    content: "\00A0\00bb";
    display: inline-block;
    height: 23px !important;
    margin: 0 0px 0 15px;
    width: 5px !important;
}
.button.up.raquo:after{
    margin: 0 7px 0 3px;
    vertical-align: text-top;
}
.button.down.raquo:after{
    transform: rotate(90deg);
    
    -webkit-transform: rotate(90deg);
    -moz-transform: rotate(90deg);
    -ms-transform: rotate(90deg);
    -o-transform:rotate(90deg);
}
.button.up.raquo:after{
    transform: rotate(270deg);

    -webkit-transform: rotate(270deg);
    -moz-transform: rotate(270deg);
    -ms-transform: rotate(270deg);
    -o-transform:rotate(270deg);
}


/* PROCEED BUTTON */
.button.proceed{
    background: #EBEBEB;
    display: block;
    float: left;
    margin: 0 12px 0 0;
    transition: all .3s;
    
    -moz-transition: all .3s;
    -webkit-transition: all 0s;
    -ms-transition: all .3s;
    -o-transition: all 0s; 
}
.button.proceed:focus, .button.proceed:hover, .button.proceed:active{
    background: #F7B244;
}
.button.proceed:after{
	border-top: 15px solid transparent;
	border-bottom: 15px solid transparent;	
	border-left: 13px solid transparent;
	bottom: auto;
	content: "";
    display: block;
    float: left;
    height: 0;
    left: auto;
    position: absolute;
    right: 0px;
    top: 0;
    transition: all 0s;
    transform: rotate(0.05deg);
	width: 0;  
	
    -moz-transition: all .3s;
    -webkit-transition: all 0s;
    -ms-transition: all .3s;
    -o-transition: all 0s;
    
    -webkit-transform: rotate(0.05deg);
    -moz-transform: scale(1.1);
    -o-transform: rotate(0.05deg);
}
.button.gray.proceed:after{
    border-left-color: #EBEBEB;
}
.button.gray.proceed:focus:after, .button.gray.proceed:hover:after, .button.gray.proceed:active:after{
    border-left-color: #F7B244 !important;
}

.button.orange.proceed{
    background: #F88C13;
}
.button.orange.proceed:focus, .button.orange.proceed:hover, .button.orange.proceed:active{
    background: #F7B244;
}
.button.orange.proceed:focus:after, .button.gray.proceed:hover:after, .button.gray.proceed:active:after{
    border-left-color: #EBEBEB;
}



/* BACK BUTTON */
.button.back{
    background: #EBEBEB;
    box-shadow: -3px 3px 12px -4px #A2A2A2;
    display: block;
    float: left;
    height: 25px;
    line-height: 25px;
    margin: 0 0 0 9px;
    transition: all 0s;
    
    -moz-box-shadow: -3px 3px 12px -4px #A2A2A2;
