/* CONTENT + COLOURED SECTIONS */
section.white       { background: #FAFAFA;  }
section.gray        { background: #F3F3F3;  }
section.content p   { text-align: justify;  }
section.topRated    { background: #E1E1E1;  }
section.secondHome  { background: #FAFAFA;  }
section#newsFooter  { background: #F0F0F0; padding-top: 15px; }

article {
    margin-bottom: 20px;
}

section.white span.step:after   { border-left-color: #FAFAFA; }
section.gray span.step:after    { border-left-color: #F3F3F3; }

section.content{
    box-shadow: inset 0px 5px 10px -5px #333;
    -moz-box-shadow: inset 0px 5px 10px -5px #333;
    -webkit-box-shadow: inset 0px 5px 10px -5px #333;
    -o-box-shadow: inset 0px 5px 10px -5px #333;
    -ms-box-shadow: inset 0px 5px 10px -5px #333;
}

/* H? */
h1{
    color: #353535;
    font-family: Ubuntu, sans-serif;
    font-size: 2.33em;
    font-weight: 400;
    line-height: 200%;
    margin: 20px 0 0;
    text-align: left;
}
h2{
    color: #353535;
    font-family: Ubuntu, sans-serif;
    font-size: 2em;
    font-weight: 400;
    line-height: 200%;
    margin: 20px 0 0;
    text-align: left;
}
h3{
    color: #353535;
    font-family: Ubuntu, sans-serif;
    font-size: 1.33em;
    font-weight: 300;
    line-height: 200%;
    margin: 20px 0 0;
    text-align: left;
}
h4{
    color: #353535;
    font-family: Ubuntu, sans-serif;
    font-size: 1.16em;
    font-weight: 500;
    line-height: 110%;
    margin: 20px 0;
    text-align: left;
}
h5{
    color: #353535;
    font-family: Ubuntu sans-serif;
    font-size: 1em;
    font-weight: 500;
    line-height: 110%;
    text-align: left; 
}

/* HEADING NOTFOUND */
.notFound{
    color: #F00;
}

/* P HEADINGS */
p.head{
    color: #353535;
    font-family: Ubuntu, sans-serif;
    text-align: left;
}
p.head1{
    font-size: 2.33em;
    font-weight: 400;
    line-height: 200%;
    margin: 20px 0 0;
}
p.head2{
    font-size: 2em;
    font-weight: 400;
    line-height: 200%;
    margin: 20px 0 0;
}
p.head3{
    font-size: 1.33em;
    font-weight: 300;
    line-height: 200%;
    margin: 20px 0 0;
}
p.head4{
    font-size: 1.16em;
    font-weight: 500;
    line-height: 110%;
    margin: 20px 0;
    text-align: left;    
}
p.head5{
    font-size: 1em;
    font-weight: 500;
    line-height: 110%;
}
p.head a:hover{
    color: #F7B244;
}

/* P.HEAD special */
p.head.head1 + form#form_information {
  margin: 10px 0;
}

/* UL */
ul, ol{
    list-style-position: inside;
    margin: 0;
    overflow: hidden;
    padding: 0;
}

ul.li-gt li{
    list-style: none;
}

ul.li-gt a:before{
    /*
    color: #F7B244;
    content: "\003E";
    font-family: Ubuntu, sans-serif;
    font-size: 1.33em;
    font-weight: 400;
    margin: 0 5px 0 -5px;
    */
    background: url('../graphics/website/sprites/icons.png?version=1') -70px -220px no-repeat transparent;
    content: "\00A0";
    display: inline-block;
    margin: 0 5px 0 -5px;
    width: 10px;
}

ul.error_list > li{
    color: #FF0000;
    list-style-type: none;
}
ul.error_list > li:before{
    content: "\00BB\00A0";
}


/* EM */
em{
    color: #2C75A6;
    font-style: normal;
    font-weight: bold;
}

/* P */
p{
    margin: 10px 0 0;
}
p.light{
    color: #BBB !important;
    font-size: 0.9em;
}

p.light.lastVisited {
    color: #F7B244 !important; 
    font-size: 1.1em;
    font-weight: bold;
}

/* IMG */
img{
    border: none;
    outline: none;
}

img.loader{
    background: #FFF;
    color: transparent;
    display: inline-block;
    pointer-events: none;
}
img.loader.big{
    background: #FFF;
}
img.loader.white{
    background-color: #FFF;
}
img.loader_64x64{
    background: url('../graphics/loader_64x64.gif') 50% 50% no-repeat scroll transparent;
    border: 0;
}


/* EMPLOYEE PLACEHOLDER */
img.employeePlaceholder{
    background: url('../graphics/website/employee_placeholder.jpg') 0 0 no-repeat scroll transparent;
}
img.employeePlaceholder.small{
    background-position: 30% 35%;
}


/* A */
a{
    color: inherit;
    cursor: pointer;
    outline: none;
    text-decoration: none;
}
a:hover, a:active, a:focus, 
a.blackUrl:hover, a.blackUrl:active, a.blackUrl:focus{
    color: #F7B244;
}
a.blackUrl{
    color: #000;
}

a > img{
    pointer-events: auto !important;
}


/* CAPTCHA */
a.a_captcha_reload{
    background: #F3F3F3;
    border: solid 1px #D4D4D4;
    border-radius: 3px;
    display: block;
    float: left;
    height: 28px;
    padding: 1px;
    overflow: hidden;
    width: 128px;
}
a.a_captcha_reload > img{
    display: block;
    float: left;
    margin: 0px;
    padding: 0;
}
a.a_captcha_reload:hover, a.a_captcha_reload:active, a.a_captcha_reload:focus{
    border-color: #F7B244;
}
a.a_captcha_reload:hover > img, a.a_captcha_reload:focus > img{
    opacity: 0.3;
}


/* A ADD (ACCOUNT) */
a.add{
    background-color: #ffae0a;
    color: #FFF;
    display: block;
    float: left;
    height: 12px;
    line-height: 12px;
    margin: 0;
    padding: 0;
    text-align: center;
    width: 12px;
}
a.add:hover, a.add:focus, a.add:active{
    background-color: #F88C13;
}


/* P */
p{
    word-break: break-word;
}
p a{
    color: #F88C13;
}


/* A hidden info */
a.hiddenInfo{
    cursor: default;
    font-weight: bold;
    overflow: visible !important;
    pointer-events: none;
    text-overflow: clip;
    white-space: normal;
}


/* HIDDEN DIV*/
div.hidden{
    display: none !important;
}

/* HIDDEN */
.hidden{
    visibility: hidden;
}

/* SUGGESTIONS */
.suggestions{
    position: absolute;
}
#div_suggestions{
    visibility: hidden;
}


/* DIV PROGRESSBAR */
.progressBar{
    background: #FFF;
    border: 1px #D6D6D6 solid;
    border-radius: 3px;
    float: left;
    padding: 1px;
    position: relative;
    width: 100%;
}
.progressBar > div.bar{
    background-color: #B4F5B4;
    background-image: url('../graphics/website/progress.png');
    background-repeat: repeat;
    border-radius: 3px;
    float: left;
    height: 20px;
    margin: 0;
    padding: 0;
    position: absolute;
    width: 0%;
}
.progressBar > div.percent{
    height: 20px;
    line-height: 20px;
    margin: 0;
    padding: 0;
    text-align: center;
    width: 100%;
}


/* TIME */
time,
time:hover, time:active,
*:hover time, *:active time{
    color: #7F7F7F;
    display: inline;
    font-family: Arial, sans-serif;
    font-size: 0.8em;
    font-weight: 300;
}
time.icon:before{
    background: url('../graphics/website/sprites/icons.png?version=1') -72px -355px no-repeat transparent;
    content: "";
    display: inline-block;
    height: 11px;
    margin: 0 4px 0 0;
    vertical-align: text-bottom;
    width: 11px;
}

/* MARK */
mark{
    background: transparent;
    color: #D4D4D4;
    display: inline-block;
    font-size: 1em;
    padding: 0 2px;
}
mark:before{
    content: "(";
}
mark:after{
    content: ")";
}


/* SELECT */
select{
    appearance: none;
    background: #F4F4F4;
    border: 1px #D4D4D4 solid;
    border-bottom-width: 2px;
    border-radius: 3px;
    color: #353535;
    cursor: pointer !important;
    float: none;
    outline: none;
    padding: 5px 30px 5px 10px;
    white-space: nowrap;
    
    -webkit-appearance: none;
    -moz-appearance: none;
    -o-appearance: none;
    -ms-appearance: none;
    
    -moz-padding-end: 10px;
}
select:active, select:hover{
    background: #FFF;
}
select option{
    outline: none;
}

/* INPUT */
input, textarea{
    outline: none;
}
input[type=text], input[type=password]{ 
    border: 1px #D4D4D4 solid;
    border-bottom-width: 2px;
    border-radius: 3px;
    display: inline-block;
    padding: 0 12px;
    height: 30px;
}
input[type=submit]{
    background-color: #F88C13 !important;
    border: 0;
    border-bottom-width: 2px;
    border-radius: 3px;
    cursor: pointer;
    height: 30px;
}
input[type=submit].shadow{
    border-bottom-width: 2px;
    box-shadow: 0 6px 10px -4px #A2A2A2;
    color: #FFF;
    font-family: Ubuntu, sans-serif;
    font-weight: 400;
    font-size: 14px;
    padding: 0 10px;
    
    -moz-box-shadow: 0 6px 10px -4px #A2A2A2;
    -webkit-box-shadow: 0 6px 10px -4px #A2A2A2;
    -ms-box-shadow: 0 6px 10px -4px #A2A2A2;
    -o-box-shadow: 0 6px 10px -4px #A2A2A2;
}
input[type=submit]:hover, input[type=submit]:active, input[type=submit]:focus{
    background-color: #F7B244 !important;
    box-shadow: 0 6px 10px -4px #353535;
    
    -moz-box-shadow: 0 6px 10px -4px #353535;
    -webkit-box-shadow: 0 6px 10px -4px #353535;
    -ms-box-shadow: 0 6px 10px -4px #353535;
    -o-box-shadow: 0 6px 10px -4px #353535;
}
input[type=submit].blue{
    background: #2C75A6 !important;
    color: #FFF !important;
}
input[type=submit].blue:focus, input[type=submit].blue:hover, input[type=submit].blue:active{
    background: #6BB4D9 !important;
}

input[type=checkbox]:active + label, input[type=radio]:active + label,
input[type=checkbox]:focus + label, input[type=radio]:focus + label{
    color: #F7B244;
}

input[type=text]:focus, input[type=password]:focus,
textarea:focus{
    border-color: #F7B244 !important;
    box-shadow: 0 0 10px 0px #F7B244;
    
    -moz-box-shadow: 0 0 10px 0px #F7B244;
    -webkit-box-shadow: 0 0 10px 0px #F7B244;
    -ms-box-shadow: 0 0 10px 0px #F7B244;
    -o-box-shadow: 0 0 10px 0px #F7B244;
}

/* INPUT FILE */
input[type=file]:focus, input[type=file]:hover{
    color: #F7B244;
}

/* */
.posRel{
    position: relative;
}


/* INPUT SPECIAL WIDTHS */
input.w50{  width: 24px;    }
input.w60{  width: 34px;    }
input.w70{  width: 44px;    }
input.w115{ width: 89px;    }
input.w130{ width: 104px;   }
input.w205{ width: 179px;   }
input.w220{ width: 194px;   }
input.w230{ width: 204px;   }
input.w280{ width: 254px;   }
input.w295{ width: 270px;   }
input.w385{ width: 359px;   }
input.w380{ width: 354px;   }


/* SPECIAL INPUT */
input.currency{
    text-align: right;
    text-indent: 10px;
}
input.currency + span,
input.datepicker + span{
    background: #FFF;
    bottom: auto;
    display: block;
    float: left;
    font-family: Ubuntu, sans-serif;
    font-size: 1.2em;
    height: 30px;
    left: 3px;
    line-height: 30px;
    position: absolute;
    min-width: 20px;
    top: 8px;
    text-align: center;
    z-index: 1;
}
input.datepicker{
    text-align: left;
}
input.datepicker + span{
    background: url('../graphics/website/sprites/admin.png?version=1') 0px 0px no-repeat scroll #FFF;
    left: auto;
    height: 20px;
    margin: 5px 5px 0 0;
    overflow: hidden;
    text-indent: -150000px;
}
input.datepicker + span:hover, input.datepicker + span:focus,
input.datepicker:hover + span, input.datepicker:active + span, input.datepicker:focus + span{
    background-position: -20px 0; 
}


/* TEXTAREA */
textarea{
    border-radius: 3px;
}

/* SPAN FAVORITE ICON */
span.favorites{
    background: url('../graphics/website/sprites/icons.png?verison=1') -70px -653px no-repeat transparent;
    display: inline-block;
    width: 15px;
}
li:hover span.favorites,
li:active span.favorites,
li:focus span.favorites,
li.current span.favorites{
    background: url('../graphics/website/sprites/icons.png?verison=1') -70px -703px no-repeat transparent;
}

/* FAVORITES */
input.favorite,{
    appearance: none;
    background: url('../graphics/website/sprites/icons.png?verison=1') -19px -402px no-repeat transparent;
    border: 0px solid transparent;
    cursor: pointer;
    display: block;
    height: 10px;
    outline: none;
    overflow: hidden;
    text-indent: -150000px;
    width: 10px;
    
    -webkit-appearance: none;
    -moz-appearance: checkbox-container;
    -ms-appearance: none;
    -o-appearance: none;
}
input.favorite:checked,
input.favorite:hover, input.favorite:active, input.favorite:focus{
    background-position: -74px -402px;
}

/* FAVORITES (A) */
a.a_icon_favorite{
    background: url('../graphics/website/sprites/icons.png?verison=1') -19px -402px no-repeat transparent;
    cursor: pointer;
    display: block;
    height: 10px;
    outline: none;
    overflow: hidden;
    text-indent: -150000px;
    width: 10px;
}
a.a_icon_favorite:hover, a.a_icon_favorite:active, a.a_icon_favorite:focus{
    background-position: -74px -402px;
}
a.a_icon_defavorite{
    background-attachment: scroll;
    background-color: transparent;
    background-image: url('../graphics/website/sprites/icons.png?verison=1');
    background-position: -74px -402px;
    background-repeat: no-repeat;
    display: block;
    height: 10px;
    outline: none;
    overflow: hidden;
    text-indent: -150000px;
    width: 10px;
}


/* LABELS */
label{
    font-family: Ubuntu, sans-serif;
    font-size: 1.1em;
    font-weight: 500;
}

/* SPECIAL */
.bold{
    font-weight: bold;
}
.ellipsis{
    display: inline-block;
    margin: 0;
    overflow: hidden;
    padding: 0;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100%;
}
.lh-30px{
    line-height: 30px;
}
.no-margin{
    margin: 0 !important;
}
.mb-min-10{
    margin-bottom: -10px !important;
}
.mb-min-11{
    margin-bottom: -11px !important;
}
.ml-0px{
    margin-left: 0px !important;
}
.mb-10px{
    margin-bottom: 10px !important;
}
.mt-0px{
    margin-top: 0px !important;
}
.mt-2px{
    float: left;
    margin-top: 2px !important;
    max-width: 530px;
}
.mt-5px{
    margin-top: 5px !important;
}
.mt-10px{
    margin-top: 10px !important;
}
.mt-20px{
    margin-top: 20px !important;
}
.ml-20px{
    margin-left: 20px !important;
}
.mh-90px{
    min-height: 90px !important;
}
.mh-90px > .wrapper h1{
    margin-top: 30px !important;
}
.mb-20px{
    margin-bottom: 20px !important;
}
.pt-20px{
    padding-top: 20px !important;
}
.pt-10px{
    padding-top: 15px !important;
}
.pb-20px{
    padding-bottom: 20px !important;
}

/* TABLE.TABLE (a default table) */
table.table{
    background: #FFF;
    border: solid 1px #D6D6D6;
    border-bottom-width: 3px;
    border-collapse: collapse;
    color: #7F7F7F;
    margin: 20px 0;
    max-width: 940px;
    width: 100%;
}
table.table tbody{
    display: block;
    padding: 0 0 20px;
}
table.table tr.thead{
    border-bottom: solid 1px #000;
    color: #000;
}
table.table tbody tr.thead ~ tr{
    background: #FFF;
    border-bottom: solid 1px #D6D6D6;
}
table.table th{
    font-family: Ubuntu, sans-serif;
    font-size: 1.5em;
}
table.table th, table.table td{
    padding: 20px 0px 5px 20px;
    text-align: left;
    width: 40%;
}
table.table th, table.table td,
table.table th + th, table.table td + td{
    max-width: 284px;
    overflow: hidden;
    text-overflow: ellipsis;
}
table.table td{
    padding-top: 5px;
    white-space: nowrap;
}
table.table th + th, table.table td + td{
    padding-left: 15px;
}
table.table th + th ~ th, table.table td + td ~ td{
    max-width: auto;
    width: auto;
}
table.table th.last, table.table td.last{
    padding-right: 20px;
}
table.table tbody tr.thead ~ tr:hover, table.table tbody tr.thead ~ tr:active{
    background-color: #ffeacc; /*#F7B244*/
    border-bottom: solid 1px #ffae0a;    
}
table.table a.actions{
    background-attachment: scroll;
    background-color: transparent;
    background-image: url('../graphics/website/sprites/admin.png?verion=1');
    background-repeat: no-repeat;
    display: inline-block;
    float: left;
    height: 20px;
    margin: -2px 15px 0 0;
    overflow: hidden;
    text-indent: -150000px;
    width: 20px;
}


/* SORTABLE ACTIONS */
.sortable a.action{
    background-attachment: scroll;
    background-color: transparent;
    background-image: url('../graphics/website/sprites/admin.png?verion=1');
    background-repeat: no-repeat;
    display: inline-block;
    float: left;
    height: 20px;
    margin: -2px 15px 0 0;
    overflow: hidden;
    text-indent: -150000px;
    width: 20px;
}
.sortable a.action.edit{
    background-position: 0px -40px;
}
.sortable a.action.edit:hover, .sortable a.action.edit:active, .sortable a.action.edit:focus{
    background-position: -20px -40px;
}
.sortable a.action.delete{
    background-position: 0px -60px;
}
.sortable a.action.delete:hover, .sortable a.action.delete:active, .sortable a.action.delete:focus{
    background-position: -20px -60px;
}


/* TABLES */
table.specs{
    border-collapse: collapse;
    margin: 20px 20px 0 20px;
}
table.specs tr{
    padding: 0;
}
table.specs th{
    border-bottom: dotted 1px #5f5F5F;
    font-weight: normal;
    overflow: hidden;
    padding: 3px 0 0;
    text-align: left;
    text-overflow: ellipsis;
    vertical-align: top;
    white-space: nowrap;
}
table.specs td{
    border-bottom: dotted 1px #5f5F5F;
    color: #2C75A6;
    font-weight: normal;
    padding: 3px 0 0;
    text-align: right;
    vertical-align: top;
}
table.specs td sup{
    display: inline-block;
    float: none;
    font-size: xx-small;
    margin: -4px 0 0 0 !important;
    width: auto;
}


/* FORMS */
table.form + table.form{
    margin-top: 30px;
}
table.form td{
    padding: 10px 0 10px 0;
    vertical-align: top;
}
table.form th{
    padding: 10px 0 10px 0;
    text-align: left;
    vertical-align: top;
    white-space: nowrap;
}
table.form th label{
    display: inline-block;
    line-height: 32px;
}
table.form input[type=text], table.form textarea{
    border: solid 1px #D4D4D4;
    margin-top: 2px;
}



/* SPECIAL */
::-webkit-input-placeholder { color: #F88C13; }
:-moz-placeholder           { color: #F88C13; }
::-moz-placeholder          { color: #F88C13; }
:-ms-input-placeholder      { color: #F88C13; }
::-ms-clear:                { display: none;  }

.grayPlaceholderText::-webkit-input-placeholder { color: #7F7F7F; }
.grayPlaceholderText:-moz-placeholder           { color: #7F7F7F; }
.grayPlaceholderText::-moz-placeholder          { color: #7F7F7F; }
.grayPlaceholderText:-ms-input-placeholder      { color: #7F7F7F; }

/* DEVIDER */
div.devider{
    background: transparent;
    float: left;
    height: 1px;
    margin: -1px 0 0;
    width: 100%;
}

div.wrapperDevider{
    clear: both;
    float: none;
    height: 1px;
    margin: -1px 0 59px;
    width: 100%; 
}


/* COLUMNS (+HALF) (IN FORMS) */
div.column{
    display: block;
    float: left;
    margin: 0;
    padding: 0;
    width: 940px;
}
div.column.half{
    width: 460px;
}
div.column + div.column{
    margin-left: 20px;
}
div.column > div{
    background: #FFF;
    border: solid 1px #D6D6D6;
    border-bottom-width: 3px;
    color: #7F7F7F;
    float: left;
    margin-top: 20px;
    padding: 0px 20px 15px;
    width: 898px;
}
div.column.half > div{
    width: 418px;
}
div.column > div.orange{
    border-color: #F7B244;
}


/* SUP */
sup{
    color: #7F7F7F;
    display: block;
    float: left;
    margin-bottom: 15px;
    font-size: 1em;
    font-weight: normal; /* bold */
    text-align: right;
    width: 100%;
}
sup .button{
    display: inline-block;
    float: none;
    margin: 0 0 0 15px;
}
sup strong{
    color: #000;
}

/* NAV */
nav, nav > ul, nav > ul > li, nav a{
    display: block;
    float: left;
    margin: 0;
    padding: 0;
}

/* FLOATING */
.float_right    { float: right !important; }
.float_left     { float: left !important; }

/* TITLES */
.title{
    font-family: Ubuntu, sans-serif;
    font-weight: 500;
}

/* DROPDOWN */
