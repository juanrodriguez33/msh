    -ms-box-shadow: -3px 3px 12px -4px #353535;
    -o-box-shadow: -3px 3px 12px -4px #353535;
}
.button.back:before{
	border-top: 12px solid transparent;
	border-bottom: 12px solid transparent;	
	border-right: 9px solid transparent;
	bottom: auto;
	content: "";
    display: block;
    float: left;
    height: 0;
    left: 0;
    margin: 1px 0 0 1px;
    position: absolute;
    right: auto;
    top: 20px;
    transition: all 0s;
	width: 0;  
	
    -moz-transition: all .3s;
    -webkit-transition: all 0s;
    -ms-transition: all .3s;
    -o-transition: all 0s; 
}
.button.gray.back:before{
