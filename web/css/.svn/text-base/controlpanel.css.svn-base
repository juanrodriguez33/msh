/* BLOCK */
section.block{
	margin-left: 20px;
}

section.block div{
	margin-top: 15px;
	padding-bottom: 15px;
	border: #CCCCCC 1px solid;
}

section.block div.uploader{
	width: auto;
	margin-left: 20px;
	padding: 0px;
	border: none;
}

section.block div.flat{
	padding-bottom: 0px;
	border-bottom: none;
}

section.full{
	width: 1160px;
}

section.full div{
	width: 1158px;
}

section.large{
	width: 767px;
}

section.large div{
    overflow: hidden;
	width: 768px; /*765px;*/
}

section.half{
	width: 570px;
}

section.half > div{
	width: 568px;
}

section.small{
	width: 373px;
}

section.small div{
	width: 371px;
}

div.column{
	width: 590px;
}

div.column_large{
	width: 787px;
}

div.column_small{
	width: 393px;
}


/* NOTIFICATION */
div#notification{
	position: fixed;
	width: 100%;
	height: 50px;
	top: 0px;
	border-bottom-style: solid;
	border-width: 3px;
	line-height: 50px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 20px;
	letter-spacing: -1px;
	text-align: center;
	display: none;
	z-index: 4;
}

div#notification.info{
	border-color: #b6bf56;
	background-color: #DADEAA;
}

div#notification.error{
	border-color: #BC3E3A;
	background-color: #DD9E9C;
}


/* STATISTICS */
div.statistics{
	position: absolute !important;
	width: 300px !important;
	margin: 0px !important;
	margin-left: 70px !important;
	margin-top: -25px !important;
	padding: 0px !important;
	padding-left: 15px !important;
	padding-right: 3px !important;
	padding-bottom: 4px !important;
	border: none !important;
	background-image: url('../graphics/controlpanel/sprite.png?version=1');
	background-position: -35px bottom;
	background-repeat: no-repeat;
	display: none !important;
	float: none !important;
	z-index: 3;
}

table tr:hover td div.statistics{
	display: block !important;
}

div.statistics h2{
	margin: 0px;
	margin-left: -15px;
	padding-left: 25px;
	padding-right: 10px;
	padding-top: 5px;
	border: none;
	background-image: url('../graphics/controlpanel/sprite.png?version=1');
	background-position: -35px -260px;
	background-repeat: no-repeat;
}

div.statistics table{
	border-top: #CCCCCC 1px solid;
}

div.statistics table tr:hover td{
	border-color: #FFC53C;
}

div.statistics table tr:last-child td{
	border: none;
}

div.statistics table tr td{
	border-color: #CCCCCC;
}

div.statistics table tr td:first-child{
	padding-left: 10px;
}


/* SUGGESTIONS */
div.suggestions{
	position: absolute !important;
	width: 360px !important;
	margin: 0px;
	margin-top: -1px !important;
	border: 1px #999999 solid;
	background-color: #FFFFFF;
	visibility: hidden;
	display: block !important;
	float: none !important;
	z-index: 2;
}

div.suggestions p{
	margin: 0px;
	margin-left: 5px;
	margin-right: 5px;
	padding: 0px;
	background: none;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	letter-spacing: normal;
	color: #999999;
}

div.suggestions ul{
	margin: 0px !important;
	padding: 0px;
	list-style: none;
	font-size: 10px;
	color: #383838;
}

div.suggestions ul li{
	width: 360px !important;
	margin: 0px !important;
	float: left;
}

div.suggestions ul li.type{
	width: 350px !important;
	padding-left: 5px;
	padding-right: 5px;
	border-bottom: 1px #FFFFFF solid;
	font-weight: bold;
}

div.suggestions ul li a{
	width: 350px;
	padding-left: 5px;
	padding-right: 5px;
	border-bottom: 1px #FFFFFF solid;
	text-decoration: none;
	color: #666666;
	display: block;
}

div.suggestions ul li a.hover{
	border-bottom: 1px #FFC53C solid;
	background-color: #F9F4DF;
}

div.suggestions ul li a span{
	text-decoration: underline;
	color: #383838;
}

div.suggestions ul li a span.count{
	text-decoration: none;
	color: #999999;
}


/* FILTER SPECIFIC */
#form_filter{
    overflow: hidden;
}
#form_filter th{
	max-width: 130px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
#form_filter li.demarcation{
    margin-top: 3px;
}
#form_filter a.a_icon_reset{
    color: #000;
}
#form_filter a.a_icon_reset:hover,
#form_filter a.a_icon_reset:active,
#form_filter a.a_icon_reset:focus{
    color: #f7b245;
}


/* FILTER SUGGESTIONS */
section.block input + br + .div_suggestions_main{
    background: #FFF;
    margin-top: 0px;
    padding-bottom: 0;
    visibility: hidden;
    width: 185px;
}
section.block input + br + .div_suggestions_main ul,
section.block input + br + .div_suggestions_main ul > li,
section.block input + br + .div_suggestions_main ul > li a{
    display: block;
    float: left;
    margin: 0;
    padding: 0;
    width: 100%;
}
section.block input + br + .div_suggestions_main ul > li a{
    border-bottom: 1px solid transparent;
    color: inherit;
    cursor: pointer;
    overflow: hidden;
    padding: 4px 0;
    text-indent: 5px;
    text-overflow: ellipsis;
    white-space: nowrap;
}
section.block input + br + .div_suggestions_main ul > li a.hover,
section.block input + br + .div_suggestions_main ul > li a:focus,
section.block input + br + .div_suggestions_main ul > li a:hover{
    border-color: #f6b244;
    background-color: #fef6e8;
    color: #F88C13;
}


/* DETAIL / EDIT */
div.flat{
    overflow-x: hidden;
}
table.form > tbody > tr > td > table > tbody > tr > th,
table.form > tbody > tr > td > table > tbody > tr > td{
    border: none !important;
    display: block;
    padding-left: 0;
}
table[role="presentation"] div{
    border: none;
    margin-top: 0;
    padding-bottom: 0;
}
table[role="presentation"] td.mceToolbar,
table[role="presentation"] td.mceToolbar td{
    background: #F0F0EE !important;
    padding-left: 0;
}
table[role="presentation"] td.mceIframeContainer{
    background-color: #FFF;
}


/* BUTTONS NEW */
a.a_button{
    border-radius: 3px;
    box-shadow: 0 6px 10px -4px #A2A2A2;
    display: block;
    float: left;
    font-family: Ubuntu, sans-serif;
    font-size: 1.1em;
    font-weight: 500;
    height: 25px;
    line-height: 25px;
    padding: 0 12px;
    text-indent: 0;
    
    -moz-box-shadow: 0 6px 10px -4px #A2A2A2;
    -webkit-box-shadow: 0 6px 10px -4px #A2A2A2;
    -ms-box-shadow: 0 6px 10px -4px #A2A2A2;
    -o-box-shadow: 0 6px 10px -4px #A2A2A2;
}
a.a_button:focus, a.a_button:hover, a.a_button:active{
    box-shadow: 0 6px 10px -4px #353535;
    text-decoration: none;
}
